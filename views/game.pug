extends layout

block content
    include header
       i.fas.fa-2x.fa-coins.coin-balance.d-flex.justify-content-center#mlp-coin
       i.fas.fa-2x.fa-money-bill.d-flex.justify-content-center#mlp-bill
        <i class="fas fa-money-bill"></i>
       #progressbar

    .demo__content
        .demo__card-cont
            for question in questions
              include card
              - var question = question
